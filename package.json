{
"name": "@prozeb/single-mind-sdk",
  "version": "0.1.0",
  "description": "SingleMind SDK for interacting with Serotonin's portfolio and wallet services",
  "type": "module",
  "main": "dist/singlemind-sdk.umd.js",
  "module": "dist/singlemind-sdk.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/singlemind-sdk.es.js",
      "require": "./dist/singlemind-sdk.umd.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "graphql-request": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.2.2"
  },
  "keywords": [
    "react",
    "sdk",
    "single-mind"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/single-mind-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/single-mind-sdk/issues"
  },
  "homepage": "https://github.com/your-username/single-mind-sdk#readme"
}
